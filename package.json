{
    "name": "kaishi-yotei-management",
    "version": "1.0.0",
    "description": "開催予定管理システム - AppSheet代替Webアプリケーション",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "dev:full": "concurrently \"npm run server\" \"npm run client\"",
        "seed": "node scripts/seedData.js",
        "expand-regions": "node scripts/expandRegionalData.js",
        "create-admin": "node scripts/createAdminUser.js",
        "build": "cd client && npm run build",
        "install-client": "cd client && npm install",
        "heroku-postbuild": "npm run install-client && npm run build",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "csv-parse": "^5.4.0",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.1",
        "googleapis": "^150.0.1",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.2",
        "nodemailer": "^6.10.1",
        "sequelize": "^6.32.1",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "concurrently": "^9.2.0",
        "jest": "^29.6.2",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "keywords": [
        "event-management",
        "schedule",
        "appsheet-alternative",
        "node.js",
        "react",
        "management-system"
    ],
    "author": "開催予定管理システム開発チーム",
    "license": "ISC"
}
